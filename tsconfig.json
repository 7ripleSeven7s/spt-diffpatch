{
  "compilerOptions": {
    "target": "ES2016", 
    "module": "Preserve", 
    "moduleResolution": "Bundler",
    "outDir": "./dist/*",
    "paths": {                                 
      "@spt-diffpatch/*": [
        "./src/*"
      ]
    }, 
    "noEmit": true,
    "noEmitOnError": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
  }
}